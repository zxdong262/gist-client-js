{
  "name": "gist-wrapper",
  "version": "0.1.0",
  "description": "Github gist API wrapper.",
  "author": "ZHAO Xudong <zxdong@gmail.com>",
  "license": "MIT",
  "main": "dist/gist-client.js",
  "module": "src/gist-client.js",
  "scripts": {
    "test": "./node_modules/.bin/standard && jest",
    "lint": "./node_modules/.bin/standard",
    "release": "webpack --progress --colors",
    "prepublishOnly": "npm run release"
  },
  "dependencies": {
    "axios": "0.19.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "dotenv-override-true": "6.2.0",
    "jest": "24.9.0",
    "standard": "14.0.2",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "./test"
    ],
    "setupFiles": [
      "dotenv-override-true/config"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "/dist/"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxdong262/gist-client-js.git"
  },
  "keywords": [
    "gist",
    "github",
    "wrapper",
    "js",
    "sdk"
  ],
  "bugs": {
    "url": "https://github.com/zxdong262/gist-client-js/issues"
  },
  "homepage": "https://github.com/zxdong262/gist-client-js#readme"
}